{
	"name": "IMA.js-skeleton",
	"version": "0.15.0-beta2",
	"description": "Isomorphic javascript application",
	"main": "build/server.js",
	"scripts": {
		"app:hello": "node_modules/.bin/gulp app:hello",
		"app:feed": "node_modules/.bin/gulp app:feed",
		"app:todos": "node_modules/.bin/gulp app:todos",
		"test": "node_modules/.bin/gulp test",
		"dev": "node_modules/.bin/gulp dev",
		"gulp": "node_modules/.bin/gulp",
		"build": "set NODE_ENV=production && node node_modules/gulp/bin/gulp build",
		"build:spa": "set NODE_ENV=production && node node_modules/gulp/bin/gulp build:spa",
		"start": "set NODE_ENV=production && node build/server"
